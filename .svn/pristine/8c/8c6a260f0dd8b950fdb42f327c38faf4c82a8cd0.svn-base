  select a.proc_inst_id,a.pdef_id,a.parammap,a.startdt,a.startuser,a.procstatus,a.stopuser,a.stopdt
  from WF_RU_INST a  left join WF_RU_INST_CONTENT c on c.proc_inst_id=a.proc_inst_id

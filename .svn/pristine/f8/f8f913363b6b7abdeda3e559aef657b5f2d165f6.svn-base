		<!--	<div style="padding-top: 30px;padding-left: 35%;padding-bottom: 20%;">
			<button type="button" onClick="sgdwSubmit()" style="width: 120px;background-color: #4385c8; color: #FFFFFF;font-size: 14px;">保存</button>
		</div>-->
		<div style="padding-top: 20px;padding-left: 5%;padding-bottom: 20%;">
			<button type="button" onClick="sgdwSubmit()" style="width: 95%; height: 45px; background-color: #007aff; color: #FFFFFF;font-size: 14px;">保存</button>
		</div>
	</div>

	<body>
		<script src="../js/mui.min.js "></script>
		<script type="text/javascript" src="../js/jquery-1.9.1.min.js"></script>
		<script type="text/javascript" src="../js/http.js"></script>
		<script type="text/javascript" src="../js/jbgc.js"></script>
		<script type="text/javascript" src="../layer/layer.js"></script>
		<script type="text/javascript ">
			mui.init();
			mui.plusReady(function initdata() {
				jbxx(); //基本信息
			});
			var dl = 0; //防频繁点击登录按钮	
			var btn = ["确认", "取消"];

			function sgdwSubmit() {
				var fkyj = $("#fkyj").val();
				var bqg_1 = $("#bqg_1").val(); //表前管
				var bqg_2 = $("#bqg_2").val();
				/*if(!user_name.exec(s)) {
					mui.alert("请输入6-15位字母数字", "温馨提示");
					return false;
				}*/
				var glg_1 = $("#glg_1").val(); //过路管
				var glg_2 = $("#glg_2").val();
				var glg_3 = $("#glg_3").val();

				var ldsx_wz = $("#ldsx").val(); //楼顶水箱
				var bfgswz = $("#bfgswz").val(); //泵房供水位置
				var shuzi = /^\d+(\.\d+)?$/;

				var ylzg = "0"; //预留支管
				var cgc = "0"; //超高层
				var lhpf = "0"; //绿化破复
				var pzdg = "0"; //配置地管
				var bqg = "0"; //表前管
				var glg = "0"; //过路管
				var ldsx = "0"; //楼顶水箱
				if($("#checkbox_5").is(':checked') == true) {
					ylzg = "1";
				}
				if($("#checkbox_6").is(':checked') == true) {
					cgc = "1";
				}
				if($("#checkbox_7").is(':checked') == true) {
					lhpf = "1";
				}
				if($("#checkbox_8").is(':checked') == true) {
					pzdg = "1";
				}
				if(fkyj == "" || fkyj == null) {
					mui.alert("反馈意见不能为空！", "温馨提示");
					return false;
				}
				if($("#checkbox_1").is(':checked') == true) {
					bqg = "1";
					if(!shuzi.exec(bqg_1)) {
						mui.alert("表前管长度需输入数字！", "温馨提示");
						return false;
					}
					if(!shuzi.exec(bqg_2)) {
						mui.alert("表前管开挖需输入数字！", "温馨提示");
						return false;
					}
				}
				if($("#checkbox_2").is(':checked') == true) {
					glg = "1";
					if(!shuzi.exec(glg_1)) {
						mui.alert("过路管托管距离需输入数字！", "温馨提示");
						return false;
					}
					if(!shuzi.exec(glg_2)) {
						mui.alert("过路管顶管管尾处面积需输入数字！", "温馨提示");
						return false;
					}
					if(!shuzi.exec(glg_3)) {
						mui.alert("过路管过路管勾面积需输入数字！", "温馨提示");
						return false;
					}
				}
				if($("#checkbox_3").is(':checked') == true) {
					ldsx = "1";
				}

				var xmid = localStorage.getItem("xmid");
				mui.confirm("您正在递交工作结果，一旦递交确认后将不能打回或再次修改，你确定要递交吗？", '温馨提示', btn, function(e) {
					if(e.index == 1) {
						return false;
					} else {
						if(dl == 0) {
							dl++;
							var csqq = $.ajax({
								type: "get",
								url: common.ip + "/XkylxController/saveSgbmFk",
								async: true,
								data: {
									"xmid": xmid,
									"bqg_1": bqg_1,
									"bqg_2": bqg_2,
									"glg_1": glg_1,
									"glg_2": glg_2,
									"glg_3": glg_3,
									"ldsx_wz": ldsx_wz,
									"bfgswz": bfgswz,
									"ylzg": ylzg,
									"cgc": cgc,
									"lhpf": lhpf,
									"pzdg": pzdg,
									"info": fkyj,
									"bqg": bqg,
									"glg": glg,
									"ldsx": ldsx,

								},
								timeout: 10000, //超时时间设置，单位毫秒
								success: function(res) {
									dl = 0;
									var jsonObject = eval("(" + res + ")");
									if(jsonObject.result = "success") {
										mui.toast("保存成功");
										//layer.msg('保存成功！', {time: 1000,});
										goto("shouye.html", 2);
									} else {
										layer.msg('保存失败！', {
											time: 1000,
										});
									}
								},
								error: function(XMLHttpRequest, status) {
									if(status == 'timeout' || status == 'error') {
										csqq.abort(); // 超时后中断请求
										dl = 0;
										mui.alert("请求超时！", "温馨提示：");

									}
								}
							});
						}
					}
				});
			}

			//给退出登录绑定事件
			mui('#fanhui').on('tap', 'h5', function(a) { //绑定所有a标签				
				var btnArray = ['否', '是'];
				mui.confirm('您确定要退出登录！', '温馨提示', btnArray, function(e) {
					if(e.index == 1) {
						goto("login.html", 2);
					}
					return false;
				})
			});
			//给其他部门标签绑定事件
			mui('#tiaozhuan').on('tap', 'a', function(a) { //绑定所有li标签
				var name = this.getAttribute("name"); //跳转类别

				if(name == "qtfk") {
					localStorage.setItem("bh", "sgb");
					goto("qtbm_fk.html", 1);
				}
			});
		</script>
	</body>

</html>
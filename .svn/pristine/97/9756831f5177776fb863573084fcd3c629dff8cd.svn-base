	<div style="padding-top: 20px;padding-left: 5%;padding-bottom: 20%;">
		<button type="button" onClick="uploadimge()" style="width: 95%; height: 45px; background-color: #007aff; color: #FFFFFF;font-size: 14px;">保存</button>
	</div>

	<body>

		<script src="../js/mui.min.js"></script>
		<script type="text/javascript" src="../js/jquery-1.9.1.min.js"></script>
		<script type="text/javascript" src="../js/http.js"></script>
		<script type="text/javascript" src="../js/index.js"></script>
		<script type="text/javascript" src="../layer/layer.js"></script>
		<script type="text/javascript">
			mui.init();
			var files = [];
			mui.plusReady(function initdata() {
				jbxx(); //基本信息
			});

			//上传保存 
			var dl = 0; //防频繁点击登录按钮	
			var btn = ["确认", "取消"];

			function uploadimge() {
				var fkyj = $("#fkyj").val();
				if(fkyj == "" || fkyj == null) {
					mui.alert("反馈意见不能为空！", "温馨提示");
					return false;
				}
				mui.confirm("您正在递交工作结果，一旦递交确认后将不能打回或再次修改，你确定要递交吗？", '温馨提示', btn, function(e) {
					if(e.index == 1) {
						return false;
					} else {
						var xmid = localStorage.getItem("xmid");

						var wa = plus.nativeUI.showWaiting();
						var server = common.ip + "/XkylxController/uploadimge_1";
						if(dl == 0) {
							dl++;
							var task = plus.uploader.createUpload(server, {
								method: "POST"
							}, function(t, status) { //上传完成
								dl = 0;
								if(status == 200) {
									mui.toast("上传成功！")
									//layer.msg('上传成功！', {time: 1000,});
									//插入本地数据库
									wa.close();
									goto("shouye.html", 2);
								} else {
									layer.msg('上传失败！', {
										time: 1000,
									});
									wa.close();
								}
							});
							task.addData("fkyj", fkyj);
							task.addData("xmid", xmid);
							for(var i = 0; i < files.length; i++) {
								var f = files[i];
								task.addFile(f.path, {
									key: f.name
								});
							}
							task.start();
						}
					}
				});
			}

			//给退出登录绑定事件
			mui('#fanhui').on('tap', 'h5', function(a) { //绑定所有a标签				
				var btnArray = ['否', '是'];
				mui.confirm('您确定要退出登录！', '温馨提示', btnArray, function(e) {
					if(e.index == 1) {
						goto("login.html", 2);
					}
					return false;
				})
			});
			//给其他部门标签绑定事件
			mui('#tiaozhuan').on('tap', 'a', function(a) { //绑定所有li标签
				var name = this.getAttribute("name"); //跳转类别		
				if(name == "qtfk") {
					localStorage.setItem("bh", "ysb");
					goto("qtbm_fk.html", 1);
				}
			});

			// 添加文件  
			var index = 1;

			function appendFile(path) {
				var bbb = {
					path: path,
					name: "uploadkey_" + index
				}
				/*	files.push(bbb);*/
				files.splice(files.length, 0, bbb);
				index++;
			}
			//删除文件
			function delFile(path) {

				for(var a = 0; a < files.length; a++) {
					if(files[a].path == path) {
						files.splice(a, 1);
						break;

					}
				}
			}
		</script>
	</body>

